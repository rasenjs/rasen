{
  "name": "@rasenjs/reactive-signals",
  "version": "0.1.2-alpha",
  "description": "TC39 Signals adapter for Rasen",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "yarn clean && ../../node_modules/.bin/tsup",
    "dev": "../../node_modules/.bin/tsup --watch",
    "clean": "rm -rf dist",
    "test": "../../node_modules/.bin/vitest run"
  },
  "keywords": [
    "rasen",
    "signals",
    "reactive",
    "adapter",
    "tc39"
  ],
  "author": "Rasen Team",
  "license": "MIT",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "dependencies": {
    "@rasenjs/core": "workspace:*",
    "signal-polyfill": "^0.2.2"
  },
  "devDependencies": {
    "@types/jsdom": "^27",
    "jsdom": "^27.2.0",
    "tsup": "^8.3.5",
    "typescript": "^5.7.2"
  }
}
