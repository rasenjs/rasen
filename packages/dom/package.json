{
  "name": "@rasenjs/dom",
  "version": "0.1.4-alpha",
  "description": "Rasen DOM renderer",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "../../node_modules/.bin/tsup --watch",
    "build": "yarn clean && ../../node_modules/.bin/tsup",
    "test": "../../node_modules/.bin/vitest --run",
    "typecheck": "../../node_modules/.bin/tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "dom",
    "renderer"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/rasenjs/rasen.git",
    "directory": "packages/dom"
  },
  "homepage": "https://github.com/rasenjs/rasen/tree/main/packages/dom",
  "bugs": {
    "url": "https://github.com/rasenjs/rasen/issues"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "author": "Rasen Contributors",
  "license": "MIT",
  "dependencies": {
    "@rasenjs/core": "workspace:*"
  }
}
