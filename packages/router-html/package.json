{
  "name": "@rasenjs/router-html",
  "version": "0.1.0-alpha",
  "description": "Rasen Router for HTML/SSR - batteries included",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "yarn clean && tsup",
    "dev": "tsup --watch",
    "test": "vitest --run",
    "test:watch": "vitest",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@rasenjs/core": "workspace:*",
    "@rasenjs/html": "workspace:*",
    "@rasenjs/router": "workspace:*"
  },
  "devDependencies": {
    "tsup": "^8.0.0",
    "typescript": "^5.0.0"
  },
  "peerDependencies": {
    "zod": "^3.22.0"
  },
  "keywords": [
    "rasen",
    "router",
    "html",
    "ssr",
    "ssg",
    "server-side-rendering"
  ],
  "author": "Rasen Team",
  "license": "MIT"
}
