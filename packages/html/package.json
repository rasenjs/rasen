{
  "name": "@rasenjs/html",
  "version": "0.1.0-alpha",
  "description": "Rasen HTML renderer for SSR/SSG",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "../../node_modules/.bin/tsup --watch",
    "build": "yarn clean && ../../node_modules/.bin/tsup",
    "test": "../../node_modules/.bin/vitest --run",
    "typecheck": "../../node_modules/.bin/tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "html",
    "ssr",
    "ssg",
    "renderer",
    "server-side-rendering"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/rasenjs/rasen.git",
    "directory": "packages/html"
  },
  "homepage": "https://github.com/rasenjs/rasen/tree/main/packages/html",
  "bugs": {
    "url": "https://github.com/rasenjs/rasen/issues"
  },
  "author": "Rasen Contributors",
  "license": "MIT",
  "dependencies": {
    "@rasenjs/core": "workspace:*"
  }
}
