{
  "name": "@rasenjs/react-native",
  "version": "0.1.0",
  "description": "Rasen React Native renderer - Direct binding to RN Fabric architecture",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "react-native",
    "fabric",
    "renderer",
    "rasen"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/rasenjs/rasen.git",
    "directory": "packages/react-native"
  },
  "homepage": "https://github.com/rasenjs/rasen/tree/main/packages/react-native",
  "bugs": {
    "url": "https://github.com/rasenjs/rasen/issues"
  },
  "author": "Rasen Contributors",
  "license": "MIT",
  "dependencies": {
    "@rasenjs/core": "workspace:*"
  },
  "peerDependencies": {
    "react-native": ">=0.72.0"
  },
  "peerDependenciesMeta": {
    "react-native": {
      "optional": false
    }
  },
  "devDependencies": {
    "@types/react-native": "^0.72.0"
  }
}
