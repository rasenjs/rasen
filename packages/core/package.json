{
  "name": "@rasenjs/core",
  "version": "0.1.2-alpha",
  "description": "Rasen core - Framework-agnostic reactive rendering",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./components": {
      "types": "./dist/components.d.ts",
      "import": "./dist/components.js",
      "require": "./dist/components.cjs"
    },
    "./test-utils": {
      "types": "./dist/test-utils.d.ts",
      "import": "./dist/test-utils.js",
      "require": "./dist/test-utils.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "../../node_modules/.bin/tsup --watch",
    "build": "yarn clean && ../../node_modules/.bin/tsup",
    "test": "../../node_modules/.bin/vitest --run",
    "typecheck": "../../node_modules/.bin/tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "reactive",
    "rendering",
    "framework"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/rasenjs/rasen.git",
    "directory": "packages/core"
  },
  "homepage": "https://github.com/rasenjs/rasen/tree/main/packages/core",
  "bugs": {
    "url": "https://github.com/rasenjs/rasen/issues"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "author": "Rasen Contributors",
  "license": "MIT",
  "devDependencies": {
    "vue": "^3.3.10"
  },
  "peerDependencies": {
    "vue": "^3.3.0"
  },
  "peerDependenciesMeta": {
    "vue": {
      "optional": true
    }
  }
}
