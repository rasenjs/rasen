{
  "name": "@rasenjs/router",
  "version": "0.1.6-alpha",
  "description": "Headless router - Framework-agnostic path matching and routing utilities",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./components": {
      "types": "./dist/components/index.d.ts",
      "import": "./dist/components/index.js",
      "require": "./dist/components/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "../../node_modules/.bin/tsup --watch",
    "build": "yarn clean && ../../node_modules/.bin/tsup",
    "test": "../../node_modules/.bin/vitest --run",
    "typecheck": "../../node_modules/.bin/tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@rasenjs/core": "workspace:^"
  },
  "peerDependencies": {
    "zod": "^3.0.0"
  },
  "keywords": [
    "router",
    "routing",
    "path-matching",
    "headless",
    "framework-agnostic"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/rasenjs/rasen.git",
    "directory": "packages/router"
  },
  "homepage": "https://github.com/rasenjs/rasen/tree/main/packages/router",
  "bugs": {
    "url": "https://github.com/rasenjs/rasen/issues"
  },
  "author": "Rasen Contributors",
  "license": "MIT",
  "sideEffects": false
}
