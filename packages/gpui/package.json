{
  "name": "@rasenjs/gpui",
  "version": "0.1.0-alpha",
  "description": "Rasen GPUI binding - GPU-accelerated UI framework adapter for Rasen",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": [
    "dist",
    "bin",
    "native"
  ],
  "bin": {
    "rasen-gpui": "./bin/rasen-gpui.cjs"
  },
  "scripts": {
    "build": "yarn clean && tsup",
    "build:native": "cargo build --release",
    "dev": "tsup --watch",
    "test": "echo 'No tests configured'",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@rasenjs/core": "workspace:^"
  },
  "devDependencies": {
    "@types/node": "^22.10.1",
    "tsup": "^8.3.5",
    "typescript": "^5.7.2"
  },
  "peerDependencies": {
    "@rasenjs/reactive-signals": "workspace:*",
    "@rasenjs/reactive-vue": "workspace:*"
  },
  "peerDependenciesMeta": {
    "@rasenjs/reactive-signals": {
      "optional": true
    },
    "@rasenjs/reactive-vue": {
      "optional": true
    }
  },
  "keywords": [
    "rasen",
    "gpui",
    "gpu",
    "ui",
    "framework",
    "rust",
    "zed"
  ],
  "author": "",
  "license": "MIT"
}
